{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/**/*",
    "tests/**/*",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "env.d.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "types": [
      "vue3-toastify/global"
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "moduleResolution": "node"
  }
}
